package Insert;

use DBI;
use strict;
use warnings;

#our $string="dbi:ODBC:dsn=project;Database=projecttest;Trusted_Connection=Yes";

our $mssql_dbh;

 #= DBI -> connect($string) or die "\n\nthe mssql connection died with the following error: \n\n$DBI::errstr\n\n";
#print "Success!!";

sub new{
	
	 my($class, %args) = @_;
 
   my $self = bless({}, $class);
 
   my $target = exists $args{dbconnection} or die "\n No dbconnection!"
   $mssql_dbh = $target;
 
   return $self;
	
}



sub InsertTrack {

}



return 1;
